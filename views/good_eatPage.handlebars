{{>header_good_eat}}


				<!-- user login part -->
		<div id="user-login-box" class = "hidden">
				<div >
					<img src="images/osuBeavers.jpg" class="login-beaver-image"><p class="user-login-greeting">Login Beavers!</p>
				</div>
				<button type="button" id="login-close-button">&times;</button>
				<div class="user-input-forms">
					<form action="" method="get" name="register">
						<input name="fullname" type="text" value="Enter your OSU ID..."  onClick="border: 1px solid #30a8da;" id="username"/>
						<input name="email" type="text" value="Enter your email address..."  onClick="border: 1px solid #30a8da;" id="mail"/>
						<input name="password" type="password" value="Enter a password..." onClick="border: 1px solid #30a8da;" id="password"/>
					</form>
				</div>

				<button class="create-account user-lonin-button" >Create Account</button>
				<button class="login-account user-lonin-button" >Login</button>
		</div>
		<!-- end of user login part -->
			
		<!-- Order food part -->
		<button type="button" id="create-order-button">
				<i id="create-order-icon" class="fa fa-cart-plus" aria-hidden="true"></i>
				<p id="order-meal">Order</p>

				<!-- <div id="create-order-icon">
					<i class="fa fa-cart-plus" aria-hidden="true"></i>
					<p id="order-meal">Order</p>
				</div> -->
				
		</button>
		<!-- end of Order food part -->







		<!-- Main -->
				<main role="main">


				
				<!-- Features -->
				<div class="fh5co-features">
					<div class="container">
						<div class="fh5co-section-title text-center">
							<h2 class="fh5co-title">Order your own good eat!</h2>
						</div>
						<div class="fh5co-spacer fh5co-spacer-sm"></div>
						  
						<div class = "check_body">
							<form method="post" action="/">


							<div class = "choice">
							  <div >
							    <div class = "choice-name">Food Restrictions</div>
							  </div>
							    <input id="celiac" type="checkbox" name="restriction" value="celiac">
							    <label for="celiac">Celiac</label>

							    <input id="nuts" type="checkbox" name="restriction" value="nuts">
							    <label for="nuts">Nuts</label>

							    <input id="vegan" type="checkbox" name="restriction" value="vegan">
							    <label for="vegan">Vegan</label>
							    <p class = "i"><I> We provide gluten free and nut free options, however we are not a gluten or nut free facility.</I></p>
							  </div>

							<div class = "choice">
							  <div class = "choice-name">Select your bread:</div>

							  <input id="white" type="radio" name="bread" value="white">
							  <label for="white">White</label>

							  <input id="wheat" type="radio" name="bread" value="wheat">
							  <label for="wheat">Wheat(organic)</label>

							  <input id="multi-grain" type="radio" name="bread" value="multi-grain">
							  <label for="multi_grain">Multi-Grain(organic)</label>

							  <input id="wheat-tortilla" type="radio" name="bread" value="wheat-tortilla">
							  <label for="wheat-tortilla">Wheat Tortilla</label>

							  <input id="sourdough" type="radio" name="bread" value="sourdough">
							  <label for="multi_grain">Sourdough</label>

							  <input id="rye" type="radio" name="bread" value="rye">
							  <label for="rye">Rye</label>

							  <input id="Gluten-Free-Bread" type="radio" name="bread" value="Gluten-Free-Bread">
							  <label for="rye">Gluten-Free-Bread(+.65)</label>


							</div>

							<div class = "choice-line">
							  <div class = "choice-name">Grilled Sandwiches:</div>
							  <label>
							    <input type="radio" id="sandwich-special" name="sandwich-special" class="grilled-sandwiches"/>
							    Sandwich
							</label>
							<label>
							    <input type="radio" id="chicken-basil-binary" name="chicken-basil-binary" class="grilled-sandwiches"/>
							    Chicken Basil Binary<span>[$5.95]</span>: <span class="description">Provolone, tomato, pesto+chicken</span>
							</label>
							<label>
							    <input type="radio" id="tuna++" name="tuna++" class="grilled-sandwiches"/>
							    Tuna ++<span>[$5.95]</span>: <span class="description">tuna, cheddar, tomato+onion
							</label>

							<label>
							    <input type="radio" id="telsa-turkey-club" name="telsa-turkey-club" class="grilled-sandwiches"/>
							    Telsa Turkey Club<span>[$5.95]</span>: <span class="description">turkey, cheddar, bacon bits, tomato, peppers + ranch dressing
							</label>

							<label>
							    <input type="radio" id="gyro" name="gyro" class="grilled-sandwiches"/>
							    Gyro<span>[$5.95]</span>: <span class="description">beef/lamb, tomato, cucumbers, onions + tzatziki sauce on pita bread
							</label>

							<label>
							    <input type="radio" id="nearly-normal-burger" name="nearly-normal-burger" class="grilled-sandwiches"/>
							    Nearly Normal Burge<span>[$6.00]</span>: <span class="description">Veggie burger, chedar, pickles, onions, tomatoes+lettuce served on a wheat bun
							</label>

							</div>

							<div class = "choice-line">
							  <div class = "choice-name">Cold Sandwiches:</div>
							  <label>
							    <input type="radio" id="veggie" name="veggie" class="cold-sandwiches"/>
							    Veggie<span>[$5.95]</span>: <span class="description">hummus, provolone, peperoncini, olives,tomatoes, onions, cucumbers, lettuce + bell peppers</span>
							</label>
							<label>
							    <input type="radio" id="egg-salad" name="egg-salad" class="cold-sandwiches"/>
							    Egg Salad<span>[$5.95]</span>: <span class="description">fresh egg salad, cheddar + lettuce</span>
							</label>
							<label>
							    <input type="radio" id="tuna" name="tuna" class="cold-sandwiches"/>
							    Tuna<span>[$5.95]</span>: <span class="description">tuna, Provolone, tomato, onion+lettuce</span>
							</label>

							</div>

							<div class = "choice-line">
							  <div class = "choice-name">Specialty Salads</div>
							  <label>
							    <input type="checkbox" id="salad-special" name="salad-special" />
							    Salad Special<span>[$0.00]</span>
							</label>
							<label>
							    <input type="checkbox" id="telesa-turkey-salad" name="telesa-turkey-salad" />
							    Tesla Turkey Salad<span>[$0.00]</span>: <span class="description">Spring mix, bacon bits, cheddar, tomatos, bell peppers + ranch dressing</span>
							</label>
							<label>
							    <input type="checkbox" id="chicken-spring-caesar" name="chicken-spring-caesar" />
							    Chicken Spring Caesar<span>[$0.00]</span>: <span class="description">Spring mix, parmesan, onion, tomatoes, croutons, caesar dressing + chicken</span>
							</label>

							</div>

							<div class = "choice">
							  <div class = "choice-name">Grilled Cheese</div>
							  <label>
							    <input type="radio" id="grilled-cheese" name="grilled-cheese" class = "grilled-chess"/>
							    grilled-cheese<span>[$3.50]</span>
							</label>
							<label>
							    <input type="radio" id="grilled-cheese-meat" name="grilled-cheese-meat" class = "grilled-chess"/>
							    Grilled Cheese + Meat<span>[$4.50]</span>
							</label>
								<div class = "choice-name">PB+J</div>
								<label>
							    <input type="radio" id="peanut-butter-jelly" name="peanut-butter-jelly" class = "pb+j"/>
							    Peanut Butter + Jelly<span>[$3.50]</span>
							</label>
							<label>
							    <input type="radio" id="peanut-butter-honey-banana" name="peanut-butter-honey-banana" class = "pb+j"/>
							    Peanut Butter, Honey + Banana<span>[$4.50]</span>
							</label>
		
							<div>
							  
							  <input id="white" type="radio" name="meat" value="turkey">
							  <label for="white">Turkey</label>

							  <input id="wheat" type="radio" name="meat" value="tuna">
							  <label for="wheat">Tuna</label>

							  <input id="multi-grain" type="radio" name="meat" value="ham">
							  <label for="multi_grain">Ham</label>

							</div> 
						   </div>

						   <!-- <div class = "inline-choice-continer"> -->
							<div class = "choice">
							   	<span class = "inline-choice">
								  <div class = "choice-name">Pickle Spear</div>
								  <input id="pickle-spear-yes" type="radio" name="pickle-spear" value="pickle-spear-yes">
								  <label for="pickle-spear">Yes</label>


								  <input id="pickle-spear-no" type="radio" name="pickle-spear" value="pickle-spear-no">
								  <label for="pickle-spear">No</label>
								</span>  

								<span class = "inline-choice">

								  <div class = "choice-name">Soup</div>
								  <input id="soup-cup" type="radio" name="soup" value="soup-cup">
								  <label for="soup">Cup</label>

								  <input id="soup-bowl" type="radio" name="soup" value="soup-bowl">
								  <label for="soup">Bowl</label>
								</span>

								<div class = "inline-choice">

								  <div class = "choice-name">Mac & Cheese</div>
								  <input id="mac-cheese-cup" type="radio" name="mac-cheese" value="mac-cheese-cup">
								  <label for="soup">Cup</label>

								  <input id="mac-cheese-bowl" type="radio" name="mac-cheese" value="mac-cheese-bowl">
								  <label for="soup">Bowl</label>
								</div>							
							</div>

							</form>
							</div>
					</div>
				</div>
				<!-- Features -->
				

		</main>
		<!-- Main -->
		